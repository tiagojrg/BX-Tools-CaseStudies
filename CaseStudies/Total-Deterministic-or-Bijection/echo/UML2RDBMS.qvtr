import Company : 'Company_MM.ecore' :: Company;
import World : 'World_MM.ecore' :: World;

transformation uml2rdbms (wrl : World, cpn : Company) {

 	top relation W2C { 
		domain wrl w:World { };
    	domain cpn c:Company { };
    }
    
    top relation P2E { 
		domain wrl p:Person { };
    	domain cpn e:Employee { };
    }

}